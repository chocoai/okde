package net.cedu.common.enums;


/**
 * 用户枚举
 * @author Jack
 *
 */
public enum UserEnum 
{
	Root(-1),			//ROOT账号
	
	StatusStart(0),		//启用
	StatusStop(1),		//停用
	ManagerYes(1),		//是区域经理
	ManagerNo(0),		//不是区域经理
	TypeAdmin(0),		//总部用户
	IsOrgTrue(1),		//总部机构
	TypeBranch(1),		//学习中心用户
	DeleteYes(1),		//已删除
	DeleteNo(0);		//未删除
	
	private final int value;
	
	UserEnum(int v){
		this.value = v;
	}
	
	public int value(){
		return value;
	}
}
