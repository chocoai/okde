package net.cedu.common.enums;



/**
 * 学习中心枚举
 * @author Jack
 *
 */
public enum BranchEnum 
{
	All(-1),	//查询所有
	Level(2),	//层级,查第几层学习中心
	Admin(1),	//系统初始化，总部学习中心ID为1
	Root(-1),	//超级用户
	Parent(0),
	Default(99);//默认查询
	private final int value;
	
	BranchEnum(int v){
		this.value = v;
	}
	
	public int value(){
		return value;
	}
}
